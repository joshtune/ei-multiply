{
  "name": "ei-multiply",
  "version": "1.0.17",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "pre-commit": "npm run build && npm run eslint:fix && npm run test",
    "code-quality": "npm run lint && npm run test",
    "lint": "npm run eslint",
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "test": "npx vitest run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run pre-commit",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshtune/ei-multiply"
  },
  "author": {
    "name": "Josh Tune",
    "url": "https://joshtune.com"
  },
  "keywords": [],
  "license": "ISC",
  "description": "This is a skeleton project to scaffold an npm package",
  "dependencies": {
    "vitest": "^0.31.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "@vitest/coverage-c8": "^0.31.4",
    "eslint": "8.42.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "typescript": "^5.1.3"
  }
}
